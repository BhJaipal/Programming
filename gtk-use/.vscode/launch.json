{
	"configurations": [
		{
			"name": "(gdb) Launch",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/aloo-edit/bin/run.exe",
			"args": [],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}/aloo-edit",
			"environment": [],
			"miDebuggerPath": "/usr/bin/gdb",
			"externalConsole": false,
			"MIMode": "gdb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				},
				{
					"description": "Set Disassembly Flavor to Intel",
					"text": "-gdb-set disassembly-flavor intel",
					"ignoreFailures": true
				}
			]
		}
	],
	"version": "2.0.0"
}
