<?xml version="1.0" encoding="UTF-8"?>
<interface>
	<object id="gtk-window" class="GtkWindow">
		<property name="title">Hello World</property>
		<child>
			<object id="app" class="GtkBox">
				<property name="name">app</property>
				<child>
					<object id="navbar" class="GtkGrid">
						<property name="name">navbar</property>
						<child>
							<object id="nav-show" class="GtkGrid">
								<property name="name">nav-show</property>
								<layout>
									<property name="column-span">5</property>
									<property name="column">0</property>
									<property name="row">0</property>
								</layout>
								<child>
									<object id="menu-bar" class="GtkButton">
										<property name="name"
											>menu-bar</property
										>
										<property name="label">â‰¡</property>
										<layout>
											<property name="column-span"
												>1</property
											>
											<property name="column">0</property>
											<property name="row">0</property>
										</layout>
									</object>
								</child>
								<child>
									<object id="menu-label" class="GtkLabel">
										<property name="name"
											>menu-label</property
										>
										<property name="label"
											>Gtk App</property
										>
										<layout>
											<property name="column-span"
												>2</property
											>
											<property name="column">1</property>
											<property name="row">0</property>
										</layout>
									</object>
								</child>
							</object>
						</child>
					</object>
				</child>
				<child>
					<object id="box" class="GtkBox">
						<property name="name">box</property>
						<child>
							<object id="grid" class="GtkGrid">
								<property name="name">button-grid</property>
								<child>
									<object id="add" class="GtkButton">
										<property name="name">add</property>
										<property name="label"
											>Add Label</property
										>
										<layout>
											<property name="column-span"
												>2</property
											>
											<property name="column">0</property>
											<property name="row">0</property>
										</layout>
									</object>
								</child>
								<child>
									<object id="remove" class="GtkButton">
										<property name="name">remove</property>
										<property name="label"
											>Remove Label</property
										>
										<layout>
											<property name="column-span"
												>2</property
											>
											<property name="column">2</property>
											<property name="row">0</property>
										</layout>
									</object>
								</child>
							</object>
						</child>
					</object>
				</child>
			</object>
		</child>
	</object>
</interface>
