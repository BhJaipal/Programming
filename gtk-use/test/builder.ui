<?xml version="1.0" encoding="UTF-8"?>
<interface>
	<object id="gtk-window" class="GtkWindow">
		<property name="title">Hello World</property>
		<child>
			<object id="box" class="GtkBox">
				<child>
					<object id="grid" class="GtkGrid">
						<child>
							<object id="add" class="GtkButton">
								<property name="name">add</property>
								<property name="label">Add Label</property>
								<layout>
									<property name="column-span">2</property>
									<property name="column">0</property>
									<property name="row">0</property>
								</layout>
							</object>
						</child>
						<child>
							<object id="remove" class="GtkButton">
								<property name="name">remove</property>
								<property name="label">Remove Label</property>
								<layout>
									<property name="column-span">2</property>
									<property name="column">2</property>
									<property name="row">0</property>
								</layout>
							</object>
						</child>
					</object>
				</child>
			</object>
		</child>
	</object>
</interface>